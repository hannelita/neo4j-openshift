#!/bin/bash

neo4j_url="$OPENSHIFT_NEO4J_DB_USERNAME:$OPENSHIFT_NEO4J_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_NEO4J_DB_PROXY_PORT/"

echo "OPENSHIFT_NEO4J_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_NEO4J_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_NEO4J_DB_USERNAME=$OPENSHIFT_NEO4J_DB_USERNAME"
echo "OPENSHIFT_NEO4J_DB_PASSWORD=$OPENSHIFT_NEO4J_DB_PASSWORD"
echo "OPENSHIFT_NEO4J_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_NEO4J_DB_PORT=$OPENSHIFT_NEO4J_DB_PROXY_PORT"
echo "OPENSHIFT_NEO4J_DB_URL=$neo4j_url"